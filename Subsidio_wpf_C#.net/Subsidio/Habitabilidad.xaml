<UserControl x:Class="OnBreak_MDT_V._2.UserControlListarCliente"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:OnBreak_MDT_V._2"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid Background="White" >
        <StackPanel  Grid.Row="0">
            <Grid Background="#FF9B9B9B">
                <Grid.RowDefinitions>
                    <RowDefinition Height="10" />
                    <RowDefinition Height="100"/>
                    <RowDefinition Height="1*"/>
                </Grid.RowDefinitions>
                <TextBlock Text="Documentos para imprimir"  Foreground="Black" FontSize="50"  Margin="84,10,120,0" Grid.Row="1"/>
            </Grid>
        </StackPanel>







        <Button x:Name="btnImprimir" Content="Imprimir" HorizontalAlignment="Left" Margin="610,380,0,0" VerticalAlignment="Top" Width="142"    Style="{DynamicResource MaterialDesignRaisedAccentButton}"/>

    </Grid>
</UserControl>
